		"@tailwindcss/typography": "^0.5.10",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/svelte": "^4.1.0",
		"@types/eslint": "^8.56.0",
		"@types/node": "^20.11.26",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
