		"unist-util-is": "^6.0.0",
		"unist-util-visit": "^5.0.0",
		"vite": "^5.0.11",
		"vitest": "^1.2.0",
		"yaml": "^2.4.1"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@types/mdast": "^4.0.3",
		"mdast-util-definitions": "^6.0.0",
		"mdast-util-directive": "^3.0.0"
	}
